# Create a directory
mkdir anchal

# Change to the directory
cd anchal

# Print working directory
pwd

# Create files
touch text.txt
touch hk.txt

# List files in the directory
ls

# Change file permissions
chmod 700 text.txt
chmod 764 text.txt

# List file details
ls -l text.txt

# Create a new user
sudo adduser aditi

# Create a new group
sudo addgroup tylt

# Add user to group
sudo usermod -aG tylt aditi

# Update password for the user
sudo passwd aditi

# Print user ID and groups
id aditi

# Create another directory
mkdir lsa

